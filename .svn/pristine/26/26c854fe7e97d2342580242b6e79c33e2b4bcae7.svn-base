							divida.setData_auto_infracao(his != null && his.getDataut()!=null &&(!his.getDataut().equalsIgnoreCase("00000000"))?new SimpleDateFormat("yyyyMMdd").parse(his.getDataut()):null); // JURProcesso_Debito
						}
