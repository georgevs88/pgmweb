	 		<listbox model="@{wVer.listProc}" height="270px" selectedItem="@{wVer.procSel}" onSelect="wVer.selecionar()">
				<listhead>
					<listheader label="INSCRIÇÃO" width="90px" align="center"/>
					<listheader label="CPF/CNPJ" width="115px" align="center"/>
					<listheader label="NR CDA" width="115px" align="center"/>
					<listheader label="VARA" width="115px" align="center"/>
					<listheader label="RAZÃO SOCIAL" align="left"/>
				</listhead>
				<listitem self="@{each=a}" value="@{a}">
					<listcell label="@{a.inscricao}" style="text-align:center;font-size:11px;" />
					<listcell label="@{a.cpf_cnpj}" style="text-align:center;font-size:11px;" />
					<listcell label="@{a.numero_certidao}" style="text-align:center;font-size:11px;"/>
					<listcell label="@{a.vara}" style="text-align:center;font-size:11px;"/>
					<listcell label="@{a.razao_social}" style="text-align:left;font-size:11px;"/>
				</listitem>
			</listbox> 
			<div align="center" height="35px" width="875x" style="width:880px">
				<button label="Petição Inicial" image="" onClick="wVer.imprimePeticao()" style="width: 150px;height: 30px;font-size: 16px;border-radius: 5px;border-color: navy;font-weight: bold;"/>
				<button label="CDA" onClick="wVer.imprimeCDA()" style="width: 150px;height: 30px;font-size: 16px;border-radius: 5px;border-color: navy;font-weight: bold;"/>
			</div>
			<grid> 
	 			<columns> 
	 				<column label="" align="right" width="140px" /> 
	 				<column label="" align="left" width="170px"/>
	 				<column label="" align="right" width="100px" /> 
	 				<column label="" align="left" width="170px"/>
	 				<column label="" align="right" width="120px" /> 
	 				<column label="" align="left" width="170px"/>				 
	 			</columns> 
	 			<rows>
	 				<row>
	 					<label value="Inscrição:"/>
	 					<textbox value="@{wVer.procSel.inscricao}" readonly="true"/>
	 					<label value="CPF/CNPJ:"/>
	 					<textbox value="@{wVer.procSel.cpf_cnpj}" readonly="true"/>
	 					<label value="Número CDA:"/>
	 					<textbox value="@{wVer.procSel.numero_certidao}" readonly="true"/>
	 				</row>
	 				<row>
	 					<label value="Razão Social:"/>
	 					<textbox value="@{wVer.procSel.razao_social}"/>
	 					<label value="VARA:"/>
						<combobox id="vara" readonly="true" width="155px" selectedItem="@{wVer.procSel.vara}">
							<comboitem value="E" label="ESTADUAL" />
							<comboitem value="F" label="FEDERAL" />
						</combobox>
	 				</row>
	 			</rows>
	 		</grid>
			<listbox model="@{wVer.listDiv}" selectedItem="@{wVer.divSel}">
				<listhead>
					<listheader label="CODDEB" width="90px" align="center"/>
					<listheader label="DESCRIÇÃO DÉBITO" width="115px" align="left"/>
					<listheader label="EXERCÍCIO" width="115px" align="center"/>
					<listheader label="DATA VENCIMENTO" width="115px" align="center"/>
					<listheader label="VALOR TOTAL" align="right"/>
				</listhead>
				<listitem self="@{each=d}" value="@{d}">
					<listcell label="@{d.cod_tipo_debito}" style="text-align:center;font-size:11px;" />
					<listcell label="@{d.descricao_tipo_debito}" style="text-align:left;font-size:11px;" />
					<listcell label="@{d.exercicio}" style="text-align:center;font-size:11px;" />
					<listcell label="@{d.data_vencimento}" style="text-align:center;font-size:11px;"/>
					<listcell label="@{d.valor_total_calc}" style="text-align:right;font-size:11px;"/>
				</listitem>
			</listbox>
