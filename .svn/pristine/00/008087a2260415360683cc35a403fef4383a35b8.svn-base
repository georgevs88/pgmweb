					//ENDERECO CORRESP
					processo.setEnd_corresp(dadosImovel.get("END_CORRESP")!=null?dadosImovel.get("END_CORRESP").toString():null);

					new Projex_ProDAO().updProcessoAntigo(processo);
					
					//ATUALIZA AS DIVIDAS
					List<Projex_DivVO> listDiv = new Projex_DivDAO().getDivByProcesso(processo.getId_processo());
					for(Projex_DivVO div : listDiv){
						
						DebVO debTIQU = new DebDAO().getRegByCod(div.getCodigo_trdeb());
