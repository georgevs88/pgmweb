							switch (div.getExercicio()) {
								case 1998: div.setOrigem("LEI 3.428 DE 24/12/1997, DECRETO 7.587 DE 21/01/1998"); break;
								case 1999: div.setOrigem("LEI 3.594 DE 14/12/1998, DECRETO 7.756 DE 28/12/1998"); break;
								case 2000: div.setOrigem("LEI 3.702 DE 21/12/1999, DECRETO 7.956 DE 29/12/1999"); break;
								case 2001: div.setOrigem("LEI 3.835 DE 28/12/2000, DECRETO 8.126 DE 28/12/2000"); break;
								case 2002: div.setOrigem("LEI 3.916 DE 17/12/2001, DECRETO 8.362 DE 26/12/2001"); break;
								case 2003: div.setOrigem("LEI 3.993 DE 12/12/2002, DECRETO 8.577 DE 20/12/2002"); break;
								case 2004: div.setOrigem("LEI 4.112 DE 04/12/2003, DECRETO 8.834 DE 19/12/2003"); break;
								case 2005: div.setOrigem("LEI 4.240 DE 06/12/2004, DECRETO 9.078 DE 08/12/2004"); break;
								case 2006: div.setOrigem("LEI 4.343 DE 29/11/2005, DECRETO 9.462 DE 12/12/2005"); break;
								case 2007: div.setOrigem("LEI 4.422 DE 08/12/2006, DECRETO 9.796 DE 14/12/2006"); break;
								case 2008: div.setOrigem("LEI 4.583 DE 21/12/2007, DECRETO 10.318 DE 28/12/2007"); break;
								case 2009: div.setOrigem("LEI 4.695 DE 19/12/2008, DECRETO 10.701 DE 30/12/2008"); break;
								case 2010: div.setOrigem("LEI 4.799 DE 23/12/2009, DECRETO 11.082 DE 06/01/2010"); break;
								case 2011: div.setOrigem("LEI 4.920 DE 28/12/2010, DECRETO 11.414 DE 30/12/2010"); break;
								case 2012: div.setOrigem("LEI 5.017 DE 12/12/2011, DECRETO 11.709 DE 28/12/2011"); break;
								case 2013: div.setOrigem("LEI 5.119 de 27/11/2012, DECRETO 12.087 de 28/12/2012"); break;
								case 2014: div.setOrigem("DECRETO 12.252 de 20/12/2013, DECRETO 12.253 de 20/12/2013"); break;
								case 2015: div.setOrigem("LEI 5.405 de 14/11/2014, DECRETO 12.496 de 19/11/2014"); break;
								case 2016: div.setOrigem("DECRETO 12.744 de 12/11/2015, DECRETO 12.754 de 26/11/2015"); break;
								case 2017: div.setOrigem("DECRETO 13.005 de 23/11/2016, DECRETO 13.006 de 25/11/2016"); break;
					        }
						} else if(div.getCodtrb() == 4 || div.getCodtrb() == 55 || div.getCodtrb() == 59){
							//SETAR A ORIGEM DAS MELHORIAS COM O DECRETO ESPECÍFICO DA ÁREA (TRIBUTOS 4,55,59)
							//Function RECUPERA_DECRETOS_MELHORIAS(P.INSCRICAO));
							// SE NÃO ACHAR COLOCA O DECRETO GERAL
							div.setOrigem(new ImoDAO().getOrigemMelhorias(processo.getInscricao()));
							if(div.getOrigem() == null || div.getOrigem().trim().isEmpty()){
