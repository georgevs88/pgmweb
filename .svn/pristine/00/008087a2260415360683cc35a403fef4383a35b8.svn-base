								          "<b>VLR. TOTAL:</b>" + valorMonetario(div.getValor_total_calc()));
						
						new Projex_DivDAO().updDividaAntiga(div);
					}
					count++;
				}
				
				
				new Projex_ProDAO().getSqlMap().commitTransaction();
			} catch (SQLException e) {
				e.printStackTrace();
			} finally {
				try {
					new Projex_ProDAO().getSqlMap().endTransaction();
