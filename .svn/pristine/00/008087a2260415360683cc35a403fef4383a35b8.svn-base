	public void atualizaTblOrigemParcelamento() throws SQLException{
		//LIMPA TABELA TBLORIPAR
		new ImoDAO().delOriPar();
		//INSERE OS CODDEB NESSA TABELA DA IMODEB NELA
		List<String> listCoddeb = new AdebDAO().getCoddebRem(codrem);
		Integer count = 1;
		for(String coddeb : listCoddeb){
			//INSERE NA TBLORIPAR
			System.out.println("Débito " + count + " de " + listCoddeb.size());
			new ImoDAO().insOriPar(coddeb);
			count++;
		}
	}
	
