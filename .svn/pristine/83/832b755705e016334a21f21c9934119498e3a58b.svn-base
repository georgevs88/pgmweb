			</isNotNull>
			<isNotNull property="datcad" prepend=" AND "> to_char(datcad,'dd/MM/yyyy') =
				to_char(#datcad:TIMESTAMP#,'dd/MM/yyyy')
			</isNotNull>
			<isNotNull property="nummrc" prepend=" AND "> nummrc = #nummrc:NUMERIC#
