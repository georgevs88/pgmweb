	}
	public String getVaraDesc(){
		if(vara!=null){
			if(vara.equalsIgnoreCase("E")){
				return "ESTADUAL";
			} else {
				return "FEDERAL";
			}
		} else {
			return "";
		}
	}
	public String getInscricaoFormatada() {
		if (inscricao != null){
			String ins = new Formatacao().zeroEsquerda(inscricao,11);
			return ins.substring(0, 10) + "-" + ins.substring(10, 11);
		} else {
			return "";
		}
	}
	public String getCpfCnpjFormatado() {
		if (cpf_cnpj != null){
			Integer len = cpf_cnpj.length();
			if(len == 11){
				//CPF
				return cpf_cnpj.substring(0, 3) + "." + cpf_cnpj.substring(3, 6) + "." + cpf_cnpj.substring(6, 9) + "-" + cpf_cnpj.substring(9, 11);
			} else if(len == 14){
				//CNPJ
				return cpf_cnpj.substring(0, 2) + "." + cpf_cnpj.substring(2, 5) + "." + cpf_cnpj.substring(5, 8) + "/" + cpf_cnpj.substring(8, 12) + "-" + cpf_cnpj.substring(12, 14);
			} else {
				return cpf_cnpj;
			}
		} else {
			return "";
		}
	}
	public String getStyle(){
